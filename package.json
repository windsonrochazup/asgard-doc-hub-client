{
  "name": "next-starter-ui",
  "version": "1.0.0",
  "description": "NextJs starter UI",
  "repository": "git@github.com:ZupIT/next-starter-ui.git",
  "author": "Kall√©o Pinheiro <windson.rocha@zup.com.br>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "next build",
    "start": "next start",
    "lint": "yarn prettier; next lint",
    "dev": "next dev",
    "server": "node ./mocks/server.js",
    "clean": "rimraf node_modules yarn.lock coverage .next && yarn cache clean --mirror && yarn",
    "prettier": "prettier --ignore-unknown --write src",
    "code-validator": "yarn lint-staged",
    "test": "jest --bail --passWithNoTests",
    "test:ci": "jest --bail --passWithNoTests --ci --no-cache --silent --noStackTrace --runInBand --coverage",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn test --coverage",
    "test:stage": "yarn test:ci --findRelatedTests",
    "generate": "yarn plop --plopfile generators/plopfile.js",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "commit": "git-cz",
    "prepare": "husky install",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "next": "^12.0.1",
    "next-seo": "^4.28.1",
    "nextjs-progressbar": "^0.0.11",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.3",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/node": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/runtime": "^7.15.4",
    "@next/eslint-plugin-next": "^12.0.1",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/react": "^6.3.12",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-select": "^5.0.1",
    "@types/styled-components": "^5.1.15",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-styled-components": "^1.13.3",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint": "^8.1.0",
    "eslint-config-next": "^12.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "faker": "^5.5.3",
    "git-cz": "^4.8.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "jest-canvas-mock": "^2.3.1",
    "jest-styled-components": "^7.0.4",
    "json-server": "^0.17.0",
    "lint-staged": "^11.2.6",
    "plop": "^2.7.6",
    "prettier": "^2.4.1",
    "react-select-event": "^5.3.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
    }
  },
  "engines": {
    "node": "14.x"
  }
}
